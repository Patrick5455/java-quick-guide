package java101;

public class OperatorsInJava {
	
	/* Four types of operators in java
	 * 
	 * 1. Arithmetic Operators +,-,/,*,%
	 * 2. Bitwise Operators
	 * 3. Relational Operators
	 * 4. Logical Operators
	 * */

	public static void main(String [] args) {
		
		// Arithmetic Operators in Java
		
		int a=6, b=3;
		
		int c1 = a+b;
		int c2 = a-b;
		int c3 = a*b;
		int c4 = a/b;
		
		
	}
}
